{
  "name": "framekit",
  "version": "0.1.0",
  "description": "CLI tool for installing UI sections from Framekit registry",
  "main": "dist/index.js",
  "bin": {
    "framekit": "./bin/framekit.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "framekit",
    "ui",
    "components",
    "sections",
    "nextjs",
    "react",
    "tailwind",
    "shadcn"
  ],
  "author": "Indranil Maiti",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "ora": "^8.1.1",
    "chalk": "^5.4.1",
    "prompts": "^2.4.2",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "zod": "^3.24.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.10",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.3.5",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
